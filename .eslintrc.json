{
    "extends": ["airbnb", "prettier"],
    "parserOptions": {
      "ecmaVersion": "latest",
      "sourceType": "module"
    },
    "plugins": ["prettier"],
    "rules": {
        "no-unused-vars":"warn",
        "no-console":"off",
        "no-shadow":"off",
        "spaced-comment":"warn",
        "prefer-destructuring":"off",
        "import/order":"off"
    }
  }